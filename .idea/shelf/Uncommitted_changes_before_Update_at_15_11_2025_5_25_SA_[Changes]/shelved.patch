+    color: #11998e;
+    font-size: 18px;
+    margin-bottom: 8px;
+}
+.shipping-info .change-btn {
+    display: inline-block;
+    color: #38ef7d;
+    text-decoration: none;
+    font-weight: 600;
+    margin-top: 6px;
+}
+.shipping-info .change-btn:hover {
+    text-decoration: underline;
+}
+
+.product-list h2 {
+    margin-bottom: 15px;
+    color: #11998e;
+    font-size: 18px;
+}
+.product-item {
+    display: grid;
+    grid-template-columns: 80px 1fr 100px 50px 100px;
+    align-items: center;
+    gap: 10px;
+    border-bottom: 1px solid #eee;
+    padding: 15px 0;
+}
+.product-item img {
+    width: 80px;
+    height: 80px;
+    border-radius: 8px;
+    object-fit: cover;
+}
+.product-detail {
+    display: flex;
+    flex-direction: column;
+    gap: 4px;
+}
+.product-name {
+    font-weight: 600;
+}
+.product-type {
+    font-size: 14px;
+    color: #777;
+}
+.product-price, .product-qty, .product-total {
+    text-align: center;
+}
+
+.summary {
+    margin-top: 25px;
+    border-top: 1px solid #eee;
+    padding-top: 20px;
+}
+.summary-row {
+    display: flex;
+    justify-content: space-between;
+    margin: 8px 0;
+}
+.summary-row.total {
+    font-weight: 700;
+    font-size: 18px;
+}
+.summary-row .highlight {
+    color: #11998e;
+}
+.checkout-btn {
+    width: 100%;
+    margin-top: 20px;
+    background: linear-gradient(to right, #11998e, #38ef7d);
+    color: white;
+    border: none;
+    border-radius: 8px;
+    padding: 12px 0;
+    font-size: 16px;
+    font-weight: 600;
+    cursor: pointer;
+    transition: 0.3s;
+}
+.checkout-btn:hover {
+    transform: translateY(-2px);
+    background: linear-gradient(to right, #38ef7d, #11998e);
+}
